{
  "name": "wix-micro-fix",
  "title": "Wix micro fix",
  "description": "Chrome extension",
  "version": "3.3.0",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint 'src/**/*.{js,ts,svelte}' && svelte-check",
    "lint:fix": "eslint 'src/**/*.{js,ts,svelte}' --fix",
    "zip": "npm run build && node zip.js"
  },
  "dependencies": {
    "@storeon/svelte": "^1.0.0",
    "storeon": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@rollup/plugin-replace": "^3.0.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/chrome": "^0.0.147",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-plugin-const-enum": "^1.1.0",
    "cross-zip": "^4.0.0",
    "eslint": "^7.30.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "fs-extra": "^10.0.0",
    "rollup": "^2.53.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "simple-git-hooks": "^2.4.1",
    "svelte": "^3.38.3",
    "svelte-check": "^2.2.2",
    "svelte-preprocess": "^4.7.4",
    "typescript": "^4.3.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint"
  },
  "repository": "https://github.com/shoonia/wix-micro-fix.git",
  "author": "alexanderz@wix.com",
  "license": "MIT"
}
